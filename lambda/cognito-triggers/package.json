{
  "name": "cognito-triggers",
  "version": "1.0.0",
  "description": "Lambda triggers for AWS Cognito User Pool",
  "main": "post-confirmation.js",
  "scripts": {
    "install-deps": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
